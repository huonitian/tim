/* pages/index/query.wxss */
page{font-family:"微软雅黑"; background:#f4f4f4;}
.Orderquery{} 

.Orderquery_title{background:#fff; font-size:30rpx;padding:1px 0; position:relative;}
.Orderquery_title .p{padding-left:30rpx; margin:30rpx 0; color:#999;}
.Orderquery_title .p text{color:#333; padding-left:30rpx;}
.Orderquery_title .reminder{position:absolute; right:30rpx; top:30rpx; display: inline-block; width:100rpx; border:1px #333 solid; text-align:center; line-height:50rpx; border-radius:50rpx; color:#333;}
.Orderquery_title .details{position:absolute; right:30rpx; bottom:30rpx; display: inline-block; width:150rpx; text-align:center; color:#3b9ff2; background:url(data:image/png;base64,) right center no-repeat; background-size:20rpx;}

.Orderquery .brigview_hide .details{ position: absolute;bottom:20rpx; right:20rpx;font-size:28rpx; color:#FF881D; padding-right:20px;background: url(data:image/png;base64,) right no-repeat;background-size:12px 7px;}

.Orderquery .brigview_hide{position:relative;}

.Orderquery .ul_classname{ padding-top:10rpx;}
.Orderquery .ul_classname .li{ border:1px solid #f4f4f4; position: relative;}
.Orderquery .ul_classname .li .Everyday{ border-left:1px dashed #FFD8B8; position:absolute; left:40rpx;top:0; height:100%;}
.Orderquery .ul_classname .li .Everyday .phone{width:40rpx; display:block;position:absolute; left:-22rpx;top:50%;bottom:0; width:40rpx; height:40rpx; margin-top:-20rpx; z-index:1;}
.Orderquery .ul_classname .li .System_review{font-size:28rpx; margin:20rpx 40rpx 20rpx 90rpx; padding:18rpx 20rpx;border-radius:6rpx;-moz-box-shadow:0px 0px 5px #CFCFCF; -webkit-box-shadow:0px 0px 5px #CFCFCF; box-shadow:0px 0px 5px #CFCFCF; position:relative; background: #fff}
.Orderquery .ul_classname .li .System_review .arrow{width:40rpx; display:block;position:absolute; left:-18rpx;top:50%;bottom:0; width:20rpx; height:20rpx; margin-top:-10rpx;}
.Orderquery .ul_classname .li .System_review .h1{font-size:26rpx;}
.Orderquery .ul_classname .li .System_review .Engineer{font-size:22rpx; color:#4D4D4D; padding:3px 0 0 0;}
.Orderquery .ul_classname .li .System_review .Engineer label{ color:#9C9C9C;}
.Orderquery .ul_classname .li .System_review .dianhdsad{ color:#3B9FF2;font-size:22rpx; padding:3px 0 0 0;}
.Orderquery .ul_classname .li .System_review .dianhdsad label{ color:#9C9C9C;}
.Orderquery .ul_classname .li .System_review .p{ padding-top:15rpx; color:#9C9C9C;font-size:22rpx;}
.Orderquery .ul_classname .li .time{  position: absolute;top:20rpx; right:20rpx;font-size:22rpx;color:#9C9C9C;}
.Orderquery .ul_classname .li:first-child .Everyday .cover{display:block; height:50%; position: absolute;top:0; left:-7rpx; width:10rpx; background-color:#f4f4f4;}
.Orderquery .ul_classname .li:last-child .Everyday .cover{display:block; height:50%; position: absolute;bottom:0; left:-7rpx; width:10rpx; background-color:#f4f4f4;}
.System_review .review_tishi{font-size: 24rpx;  color:#9C9C9C; padding-top:10rpx;}
.System_review .review_text .text_p{font-size:24rpx; color:#9C9C9C; padding-top:5rpx;}
.System_review .review_text .text_p text{color:#333;}
.System_review .review_text .text_p .blue{color:#78bdf5;}

.bright789_view_hide{
 display: block;
}
.bright789_view_show{
 display: none;
}
/*****只有一条数据追加add*******/
.Orderquery .ul_classname .li .add{border-left:none;}

.goback{display:inline-block; width:90rpx; height:90rpx; position:fixed; right:20rpx; bottom:20%;background:url(data:image/png;base64,) no-repeat; background-size:100% 100%;}